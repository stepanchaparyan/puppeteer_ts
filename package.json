		"base-64": "^0.1.0",
		"chai": "^4.1.2",
		"jsonpath": "^1.0.1",
		"minimist": "^1.2.0",
		"mocha": "^4.1.0",
		"mocha-testrail-reporter": "^1.0.12",
		"mochawesome": "^3.1.1",
		"node-fetch": "^2.3.0",
		"path": "^0.12.7",
		"puppeteer": "^0.13.0",
